{
    "name": "tyteck/skiloisirsdiffusion",
    "description": "Need to speak to ski loisirs diffusion soap service ?.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "frederick tyteca",
            "email": "frederick@tyteca.net"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-soap": "*",
        "vlucas/phpdotenv": "^5.0",
        "nesbot/carbon": "^2.46"
    },
    "autoload": {
        "psr-4": {
            "SkiLoisirsDiffusion\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SkiLoisirsDiffusion\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "fzaninotto/faker": "^1.9",
        "symfony/var-dumper": "^5.2",
        "mockery/mockery": "^1.4"
    }
}
